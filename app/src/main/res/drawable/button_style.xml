<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置边框颜色和宽度 -->
    <stroke
        android:width="2dp"
    android:color="?attr/ColorBtnEdge"
    />
    <!-- 设置按钮的背景颜色 -->
    <solid android:color="#11d6bb" /> <!-- 按钮的背景颜色，这里设置为白色 -->
    <!-- 设置按钮的圆角半径 -->
    <corners android:radius="8dp" /> <!-- 圆角半径，根据需要进行调整 -->
</shape>
